
public class QueueArray {
    private int[] array;
    private int last; //Index of last added item

    public QueueArray() {
	array = new int[100];
	last = -1;
    }

    public void newArray() {
	int[] tmp = new int[array.length + 100];
	for(int i=0; array[i]!=null; i++) {
	    tmp[i] = array[i];
	}
	array = tmp;
    }
    
    public void enqueue(int data) {
	try {
	    array[last+1] = data;
	}
	catch (Exception e) {
	    newArray();
	    enqueue(data);
	}
    }
}
